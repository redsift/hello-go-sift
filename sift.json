{
  "name": "hello-go-sift",
  "display-name": "Hello Go Sift",
  "description": "",
  "author": "",
  "version": "1.0.0",
  "icon": "assets/blueprint.svg",
  "interfaces": {
    "email": {
      "thread" : {
        "controller": "dist/js/controller.umd-es2015.min.js"
      },
      "client" : {
        "controller": "dist/js/email-client-controller.umd-es2015.min.js"
      }
    },
    "summary": {
      "controller": "dist/js/controller.umd-es2015.min.js"
    }
  },
  "dag": {
    "inputs":{
      "emails": {
        "gmailEmails":{
          "filter":{
            "conditions":[{
              "date": "between now and 2 days before now"
            }]
          },
          "wants": [
            "archive",
            "textBody",
            "strippedHtmlBody"
          ]
        }
      }
    },
    "nodes":[
      {
      "#": "Javascript Node",
      "implementation": {
        "javascript": "server/node1.js"
      },
      "input": {
        "bucket": "gmailEmails"
      }
    },
    {
      "#": "Go Node",
      "implementation": {
        "go": "server/node1/node1.go",
        "sandbox": "quay.io/redsift/sandbox-go:v1.9"
      },
      "input": {
        "bucket": "gmailEmails"
      }
    }
  ],
    "stores":{},
    "outputs":{}
  }
}
